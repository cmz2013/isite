                <version>${isite.version}</version>
            </dependency>
            <dependency>
                <groupId>org.isite</groupId>
                <artifactId>mybatis-starter</artifactId>
                <version>${isite.version}</version>
            </dependency>
            <dependency>
                <groupId>org.isite</groupId>
